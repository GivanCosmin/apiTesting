{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "array",
  "title": "The Root Schema",
  "items": {
    "$id": "#/items",
    "type": "object",
    "title": "The Items Schema",
    "required": [
      "id",
      "employee_name",
      "employee_salary",
      "employee_age",
      "profile_image"
    ],
    "properties": {
      "id": {
        "$id": "#/items/properties/id",
        "type": "string",
        "title": "The Id Schema",
        "default": "",
        "examples": [
          "170987"
        ],
        "pattern": "^(.*)$"
      },
      "employee_name": {
        "$id": "#/items/properties/employee_name",
        "type": "string",
        "title": "The Employee_name Schema",
        "default": "",
        "examples": [
          "a"
        ],
        "pattern": "^(.*)$"
      },
      "employee_salary": {
        "$id": "#/items/properties/employee_salary",
        "type": "string",
        "title": "The Employee_salary Schema",
        "default": "",
        "examples": [
          "3000"
        ],
        "pattern": "^(.*)$"
      },
      "employee_age": {
        "$id": "#/items/properties/employee_age",
        "type": "string",
        "title": "The Employee_age Schema",
        "default": "",
        "examples": [
          "20"
        ],
        "pattern": "^(.*)$"
      },
      "profile_image": {
        "$id": "#/items/properties/profile_image",
        "type": "string",
        "title": "The Profile_image Schema",
        "default": "",
        "examples": [
          ""
        ],
        "pattern": "^(.*)$"
      }
    }
  }
}